@model IEnumerable<StudentPortalSPA.Models.Course>

@{
    ViewData["Title"] = "Index";
}

<h1>Current Courses In the System</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Course Title
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LessonName)
            </td>
        </tr>
}
    </tbody>
</table>
